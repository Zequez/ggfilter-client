{
  "name": "client",
  "private": true,
  "version": "1.1.1",
  "description": "GGFilter client",
  "main": "",
  "scripts": {
    "start": "webpack-dev-server --hot --inline --env=dev",
    "test": "NODE_ENV=test jest",
    "debugtest": "node --debug-brk ./node_modules/karma/bin/karma start",
    "_posttest": "npm run lint",
    "dist:build": "npm run copy && ruby ./copy_version_to_gem.rb && NODE_ENV=production webpack --env=dist",
    "dist:commit": "cd ../client-gem && rake commit",
    "dist:push": "git push origin master && cd ../client-gem && git push origin master",
    "dist:deploy": "npm run dist:build && npm run dist:commit && npm run dist:push",
    "lint": "eslint ./src",
    "copy": "copyfiles -f ./src/index.html ./src/images/favicon1.png ./src/images/favicon2.png ../client-gem/app/assets/javascripts/app",
    "clean": "rimraf ../client-gem/app/assets/*",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "analyze": "webpack --json --env=dist | ./node_modules/.bin/webpack-bundle-size-analyzer",
    "fontello-sass-vars": "node src/shared/style/fontello/sass_vars.js"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/__mocks__/globalMocks.js",
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "testRegex": "spec\\.(jsx?|tsx?)$",
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif)$": "<rootDir>/__mocks__/fileMock.js",
      "^.+\\.(css|less|sass|scss)$": "identity-obj-proxy",
      "^shared/(.*)": "<rootDir>/src/shared/$1",
      "^images/(.*)": "<rootDir>/src/images/$1",
      "^src/(.*)": "<rootDir>/src/$1",
      "^helpers/(.*)": "<rootDir>/src/test/helpers/$1"
    }
  },
  "repository": "",
  "keywords": [],
  "author": "Zequez",
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.6",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@types/classnames": "^2.2.6",
    "@types/jest": "^23.3.2",
    "@types/lodash": "^4.14.118",
    "@types/node": "^10.12.9",
    "@types/react": "^16.7.6",
    "@types/react-dom": "^16.0.9",
    "@types/react-redux": "^6.0.9",
    "acorn": "^6.0.4",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-runtime": "^6.15.0",
    "circular-dependency-plugin": "^5.0.2",
    "classnames": "^2.2.5",
    "copyfiles": "^2.1.0",
    "css-loader": "^1.0.1",
    "enzyme": "^3.7.0",
    "eslint": "^5.9.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "file-loader": "^2.0.0",
    "fsevents": "^2.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "minimist": "^1.2.0",
    "node-sass": "^4.10.0",
    "null-loader": "^0.1.1",
    "object-assign": "^4.1.0",
    "open": "0.0.5",
    "phantomjs-prebuilt": "^2.1.16",
    "react-hot-loader": "^4.3.12",
    "react-test-renderer": "^16.6",
    "redux-devtools": "^3.4.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^7.1.0",
    "sinon": "^7.1.1",
    "style-loader": "^0.23.1",
    "ts-jest": "^23.1.4",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6",
    "typings-for-css-modules-loader": "^1.7.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
