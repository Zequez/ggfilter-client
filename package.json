{
  "name": "client",
  "private": true,
  "version": "0.1.0",
  "description": "GGFilter client",
  "main": "",
  "scripts": {
    "start": "node server.js --env=dev",
    "test": "jest",
    "debugtest": "node --debug-brk ./node_modules/karma/bin/karma start",
    "_posttest": "npm run lint",
    "serve": "node server.js --env=dev",
    "serve:dist": "node server.js --env=dist",
    "dist": "npm run copy && ruby ./copy_version_to_gem.rb && NODE_ENV=production webpack --env=dist && npm run dist:commit",
    "dist:commit": "cd gem && rake commit",
    "lint": "eslint ./src",
    "copy": "copyfiles -f ./src/index.html ./src/favicon.ico ./gem/app/assets/javascripts/app",
    "clean": "rimraf ./gem/app/assets/*",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "analyze": "webpack --json --env=dist | ./node_modules/.bin/webpack-bundle-size-analyzer"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "testRegex": "spec\\.(js|jsx)$",
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif)$": "<rootDir>/__mocks__/fileMock.js",
      "^.+\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "^shared/(.*)": "<rootDir>/src/shared/$1",
      "^images/(.*)": "<rootDir>/src/images/$1",
      "^src/(.*)": "<rootDir>/src/$1",
      "^helpers/(.*)": "<rootDir>/src/test/helpers/$1"
    }
  },
  "repository": "",
  "keywords": [],
  "author": "Zequez",
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-jest": "^16.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.16.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-es2015-loose-native-modules": "^1.0.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.11.6",
    "chai": "^3.5.0",
    "circular-dependency-plugin": "^2.0.0",
    "classnames": "^2.2.5",
    "compass-mixins": "^0.12.10",
    "copyfiles": "^1.0.0",
    "css-loader": "^0.25.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.8.0",
    "eslint-config-standard": "^6.0.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-promise": ">=2.0.1",
    "eslint-plugin-react": "^6.2.0",
    "eslint-plugin-standard": ">=2.0.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "istanbul": "0.4.5",
    "istanbul-instrumenter-loader": "^1.0.0",
    "jest": "^16.0.2",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.1.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-phantomjs-shim": "^1.4.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "madge": "^1.4.3",
    "minimist": "^1.2.0",
    "mocha": "^3.0.2",
    "node-sass": "^3.9.3",
    "null-loader": "^0.1.1",
    "object-assign": "^4.1.0",
    "open": "0.0.5",
    "phantomjs-prebuilt": "^2.1.7",
    "react-addons-test-utils": "^15.3.1",
    "react-hot-loader": "^3.0.0-beta.3",
    "react-test-renderer": "^15.3.2",
    "redux-devtools": "^3.3.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "susy": "^2.2.12",
    "url-loader": "^0.5.7",
    "webpack": "2.1.0-beta.25",
    "webpack-bundle-size-analyzer": "^2.2.0",
    "webpack-dev-server": "2.1.0-beta.9"
  },
  "dependencies": {
    "axios": "^0.14.0",
    "es6-promise": "^3.2.1",
    "font-awesome": "^4.6.3",
    "fsevents": "^1.0.14",
    "history": "^4.0.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.15.0",
    "react": "^15.3.1",
    "react-addons-pure-render-mixin": "^15.3.1",
    "react-addons-update": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-draggable": "^2.2.1",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "remap-istanbul": "^0.7.0",
    "reselect": "^2.5.4",
    "url-pattern": "^1.0.1"
  }
}
