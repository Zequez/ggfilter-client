  },
  "dependencies": {
    "@babel/runtime": "^7.1.5",
    "autotrack": "^2.4.1",
    "axios": "^0.18.0",
    "es6-promise": "^4.2.5",
    "history": "^4.0.0",
    "immutability-helper": "^2.0.0",
    "immutable": "^4.0.0-rc.12",
    "include-media": "^1.4.9",
    "lodash": "^4.17.11",
    "nprogress": "^0.2.0",
    "object.values": "^1.0.4",
    "postcss": "^7.0.6",
    "react": "^16.6",
    "react-addons-pure-render-mixin": "^15.4",
    "react-dom": "^16.6",
    "react-draggable": "^3.0.5",
    "react-redux": "^5.1.1",
    "react-router5": "^6.5.3",
    "redux": "^4.0.1",
    "redux-mediaquery": "^0.11.0",
    "redux-router5": "^6.0.5",
    "redux-saga": "^0.16.2",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "router5": "^6.6.2"
  }
}
