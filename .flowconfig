[ignore]
.*/node_modules/fbjs/.*

[include]

[libs]

[options]
module.name_mapper='^shared/\(.*\)' -> '<PROJECT_ROOT>/src/shared/\1'
module.name_mapper='^images/\(.*\)' -> '<PROJECT_ROOT>/src/images/\1'
module.name_mapper='^src/\(.*\)' -> '<PROJECT_ROOT>/src/src/\1'
module.name_mapper.extension='css' -> '<PROJECT_ROOT>/__mocks__/flowCss.js.flow'
module.name_mapper.extension='sass' -> '<PROJECT_ROOT>/__mocks__/flowCss.js.flow'
module.name_mapper.extension='png' -> '<PROJECT_ROOT>/__mocks__/flowImage.js.flow'
esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable
