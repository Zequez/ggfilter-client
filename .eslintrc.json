{
  "parser": "babel-eslint",
  "plugins": ["react"],
  "extends": "standard",
  "globals": {
    "expect": true,
    "describe": true,
    "it": true,
    "beforeEach": true,
    "afterEach": true,
    "localStorage": true,
    "btoa": true,
    "atob": true
  },
  "rules": {
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "spaced-comment": 0,
    "no-return-assign": 0,
    "no-fallthrough": 0,
    "no-template-curly-in-string": 0
  }
}
